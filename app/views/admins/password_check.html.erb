<%= content_for :title_tag do t('.title') end %>

<div class="dashboard">
  <div class="dashboard-container">
    <div class="container">
      <div class="row relative">
        <div class="col-xs-12 col-md-6 offset-md-3">
          <div class="form-picto">
            <i class="fa fa-key"></i>
          </div>
          <%= render "shared/field_with_button", url: admin_password_verification_path, query: "query", subquery: :password, placeholder: t('.placeholder'), btn_text: t('.btn_text'), method: :get %>
        </div>
      </div>
    </div>
  </div>
</div>
